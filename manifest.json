{
	"manifest_version": 2,
	"name": "Tilde Enhanced Up",
	"version": "0.0.2",
	"description": "A minimal startpage for your browser ",
	"developer": {
		"name": "ghetreek",
		"url": "https://github.com/ghetreek/"
	},
	"content_security_policy": "script-src 'self' https://duckduckgo.com; object-src 'self'",
	"applications": {
		"gecko": {
			"id": "teu@ghetreek",
			"strict_min_version": "57.0"
		}
	},
	"permissions": [
		"storage"
	],
	"chrome_url_overrides" : {
		"newtab": "index.html"
	}
}
